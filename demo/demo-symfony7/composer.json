{
    "name": "nowo-tech/anonymize-demo-symfony7",
    "description": "Demo project for Anonymize Bundle - Symfony 7.0 with MySQL and PostgreSQL",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1,<8.6",
        "doctrine/doctrine-bundle": "^2.8 || ^2.9 || ^2.10",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/orm": "^2.13 || ^3.0",
        "mongodb/mongodb": "^2.1",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/flex": "^2.0",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-csrf": "7.0.*",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "audit": {
            "block-insecure": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    },
    "repositories": [],
    "require-dev": {
        "nowo-tech/anonymize-bundle": "v0.0.16",
        "symfony/debug-bundle": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    }
}
